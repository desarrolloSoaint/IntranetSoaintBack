{
    "openapi": "3.0.0",
    "info": {
        "title": "Swagger API Documentation",
        "description": "Laravel",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://127.0.0.1:8000/",
            "description": "Demo API Server"
        }
    ],
    "paths": {
        "/api/register": {
            "post": {
                "summary": "Registrar usuarios",
                "operationId": "App\\Http\\Controllers\\UserController::register",
                "requestBody": {
                    "description": "Introducir Datos",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "name",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "ejemplo@mail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "Contrase√±a"
                                    },
                                    "name": {
                                        "type": "string",
                                        "format": "name",
                                        "example": "Nombre"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Usuario Registrado"
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        }
    },
    "security": [
        []
    ]
}